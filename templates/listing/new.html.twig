{% extends 'base.html.twig' %}

{% block body %}
    <div class="container mt-4">
        <h2>Create New Listing</h2>
        {{ form_start(form) }}
        {{ form_row(form.title) }}
        {{ form_row(form.description) }}
        {{ form_row(form.price) }}
        {{ form_row(form.category) }}
        <button type="submit" class="btn btn-success mt-3">Submit</button>
        {{ form_end(form) }}
    </div>
{% endblock %}
